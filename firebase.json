{
  "flutter": {
    "platforms": {
      "android": {
        "default": {
          "projectId": "txt-invitation",
          "appId": "1:873712718316:android:4804d9e9f15d0e853830c9",
          "fileOutput": "android/app/google-services.json"
        }
      },
      "dart": {
        "lib/firebase_options.dart": {
          "projectId": "txt-invitation",
          "configurations": {
            "android": "1:873712718316:android:4804d9e9f15d0e853830c9",
            "web": "1:873712718316:web:bd5584fa0b0084693830c9"
          }
        }
      }
    }
  },
  "hosting": {
    "public": "build/web",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/rsvp",
        "function": "ogPreview"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  },
  "functions": [
    {
      "source": "functions",
      "codebase": "default",
      "ignore": [
        "node_modules",
        ".git",
        "firebase-debug.log",
        "firebase-debug.*.log",
        "*.local"
      ],
      "predeploy": [
        "npm --prefix \"$RESOURCE_DIR\" run lint",
        "npm --prefix \"$RESOURCE_DIR\" run build"
      ]
    }
  ]
}
